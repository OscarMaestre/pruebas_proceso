	sql="update gaseosa set auxiliar='En paro, su ultimo contrato acabó el 16-10-2015' where dni='05924392Y'"
	db.Execute sql, dbFailOnError

		 'se hace el commit
  ws.CommitTrans

Proc_Exit:
  Set ws = Nothing
  Set db = Nothing
  Exit Function

Proc_Err:
  ws.Rollback
  MsgBox "Error actualizando: " & Err.Description
  Resume Proc_Exit
End Function
	
Public Function fun_13102015_3()
	fun_13102015_1
End Function

